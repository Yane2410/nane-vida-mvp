<svg width="512" height="512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#A78BFA;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B5CF6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" rx="110" fill="url(#grad)"/>
  
  <!-- Lotus flower petals -->
  <g transform="translate(256, 256)">
    <!-- Center circle -->
    <circle cx="0" cy="0" r="40" fill="#FFFFFF" opacity="0.95"/>
    
    <!-- Petals -->
    <ellipse cx="0" cy="-80" rx="45" ry="70" fill="#FFFFFF" opacity="0.9" transform="rotate(0)"/>
    <ellipse cx="0" cy="-80" rx="45" ry="70" fill="#FFFFFF" opacity="0.9" transform="rotate(60)"/>
    <ellipse cx="0" cy="-80" rx="45" ry="70" fill="#FFFFFF" opacity="0.9" transform="rotate(120)"/>
    <ellipse cx="0" cy="-80" rx="45" ry="70" fill="#FFFFFF" opacity="0.9" transform="rotate(180)"/>
    <ellipse cx="0" cy="-80" rx="45" ry="70" fill="#FFFFFF" opacity="0.9" transform="rotate(240)"/>
    <ellipse cx="0" cy="-80" rx="45" ry="70" fill="#FFFFFF" opacity="0.9" transform="rotate(300)"/>
    
    <!-- Inner petals -->
    <ellipse cx="0" cy="-55" rx="35" ry="50" fill="#FEF3C7" opacity="0.8" transform="rotate(30)"/>
    <ellipse cx="0" cy="-55" rx="35" ry="50" fill="#FEF3C7" opacity="0.8" transform="rotate(90)"/>
    <ellipse cx="0" cy="-55" rx="35" ry="50" fill="#FEF3C7" opacity="0.8" transform="rotate(150)"/>
    <ellipse cx="0" cy="-55" rx="35" ry="50" fill="#FEF3C7" opacity="0.8" transform="rotate(210)"/>
    <ellipse cx="0" cy="-55" rx="35" ry="50" fill="#FEF3C7" opacity="0.8" transform="rotate(270)"/>
    <ellipse cx="0" cy="-55" rx="35" ry="50" fill="#FEF3C7" opacity="0.8" transform="rotate(330)"/>
    
    <!-- Center detail -->
    <circle cx="0" cy="0" r="25" fill="#A78BFA"/>
    <circle cx="0" cy="0" r="15" fill="#FFFFFF" opacity="0.5"/>
  </g>
  
  <!-- Text -->
  <text x="256" y="420" font-family="system-ui, -apple-system, sans-serif" font-size="48" font-weight="700" fill="#FFFFFF" text-anchor="middle">NANE VIDA</text>
</svg>
